        "url": "https://zayro.github.io/"
    },
    "private": true,
    "engines": {
        "node": "14.x",
        "npm": "6.x"
    },
    "main": "index.js",
    "bin": {
        "api": "./bin/index.js"
    },
    "scripts": {
        "babel": "./node_modules/.bin/babel",
        "lint-standard": "standard",
        "lint": "eslint ./",
        "lint:fix": "eslint ./ --fix",
        "test": "mocha test/* --require @babel/register --timeout 10000 --exit",
        "test:watch": "mocha -w test/* --require @babel/register",
        "test-markdown": "mocha test/ -R markdown > doc/spec.md --recursive",
        "test-html": "mocha test/ -R doc > doc/spec.html --recursive",
        "test:coverage": "nyc --require @babel/register npm test",
        "coverage:report": "nyc report --reporter=html",
        "coverage": "nyc npm run test",
        "start": "npx gulp && node ./dist/index.js",
        "start:babel": "babel-node ./src/index.js",
        "start:dev": "env-cmd --verbose -f ./.env nodemon --exec  ./node_modules/.bin/babel-node  ./src/index.js ",
        "start:lab": "env-cmd --verbose -f ./.env.dev nodemon --exec  ./node_modules/.bin/babel-node  ./src/index.js   --config-file ./.babelrc",
        "watch": "babel src -d app --source-maps --watch",
        "build": "babel src -d dist",
        "build:min": "babel --plugins @babel/plugin-transform-runtime transform-remove-console src -d dist --presets minify",
        "production": "env-cmd -f ./.env node ./dist/index.js",
        "production:pm2": "pm2 start ./dist/index.js  --name api-app --log log",
        "seeder/db:demo": "babel-node ./postgresqlDB/seeder.js ",
        "seeder/db:apt": "babel-node ./database/postgresqlDB/seed_apt.js ",
        "redisSub": "node src/redisSub.js",
        "redisPub": "node src/redisPub.js"
    },
    "license": "ISC",
