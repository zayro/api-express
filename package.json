{
    "name": "api-backend",
    "version": "1.0.0",
    "description": "API REST ",
    "author": "Marlon Zayro Arias Vargas",
    "private": true,
    "engines": {
        "node": "14.x",
        "npm": "6.x"
    },
    "scripts": {
        "babel": "./node_modules/.bin/babel",
        "lint-standard": "standard",
        "lint": "eslint ./",
        "lint:fix": "eslint ./ --fix",
        "test": "mocha test/* --require @babel/register --timeout 10000 --exit",
        "test:watch": "mocha -w test/* --require @babel/register",
        "test-markdown": "mocha test/ -R markdown > doc/spec.md --recursive",
        "test-html": "mocha test/ -R doc > doc/spec.html --recursive",
        "test:coverage": "nyc --require @babel/register npm test",
        "coverage:report": "nyc report --reporter=html",
        "coverage": "nyc npm run test",
        "start": "env-cmd --verbose -f ./.env.dev  babel-node ./src/index.js  --config-file ./.babelrc",
        "start:dev": "env-cmd --verbose -f ./.env.dev nodemon ./src/index.js --exec babel-node  --config-file ./.babelrc",
        "watch": "babel src -d app --source-maps --watch",
        "build": "babel --plugins @babel/plugin-transform-runtime src -d dist",
        "build:min": "babel --plugins @babel/plugin-transform-runtime src -d dist --presets minify",
        "build:minify": "babel src -d dist --config-file ./.babelrc",
        "production": "node ./dist/index.js",
        "production:pm2": "pm2 start ./dist/index.js --name api-app --log log"
    },
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^3.0.6",
        "body-parser": "^1.20.0",
        "colors": "^1.3.0",
        "compression": "^1.7.4",
        "cors": "^2.8.4",
        "csurf": "^1.11.0",
        "dotenv": "^5.0.1",
        "dotenv-expand": "^5.1.0",
        "dotenv-parse-variables": "^2.0.0",
        "ejs": "^3.1.8",
        "env-cmd": "^10.1.0",
        "errorhandler": "^1.5.0",
        "express": "^4.18.1",
        "express-fileupload": "^0.3.0",
        "express-jsonschema": "^1.1.6",
        "express-jwt": "^6.1.2",
        "express-jwt-permissions": "^1.3.6",
        "express-pdf": "^1.2.2",
        "express-redis-cache": "^1.1.3",
        "express-validator": "^6.14.2",
        "helmet": "^4.6.0",
        "html2canvas": "^1.4.1",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.95.15",
        "mariadb": "^2.5.6",
        "method-override": "^2.1.3",
        "moment": "^2.29.4",
        "mongoose": "^5.13.14",
        "morgan": "^1.10.0",
        "morgan-body": "^2.6.6",
        "morgan-json": "^1.1.0",
        "multer": "^1.4.4",
        "mysql": "^2.18.1",
        "mysql2": "^2.3.3",
        "numeraljs": "^1.5.6",
        "optimist": "^0.6.1",
        "pg": "^8.7.3",
        "pm2": "^4.5.6",
        "sha256": "^0.2.0",
        "sql-query": "^0.1.27",
        "swagger-jsdoc": "^6.2.5",
        "swagger-ui-express": "^4.5.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.18.10",
        "@babel/core": "^7.18.10",
        "@babel/eslint-parser": "^7.18.9",
        "@babel/node": "^7.18.10",
        "@babel/plugin-transform-runtime": "^7.18.10",
        "@babel/preset-env": "^7.18.10",
        "@babel/register": "^7.18.9",
        "babel-preset-minify": "^0.5.2",
        "chai": "^4.3.6",
        "chai-http": "^4.0.0",
        "chalk": "^4.1.2",
        "cross-env": "^7.0.3",
        "eslint": "^7.32.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.2.0",
        "faker": "^5.5.3",
        "jwt-decode": "^3.1.2",
        "longjohn": "^0.2.12",
        "mocha": "^8.4.0",
        "mochawesome": "^6.3.1",
        "nodemon": "^1.19.4",
        "nyc": "^15.1.0",
        "settings": "^0.1.1",
        "standard": "^16.0.4",
        "supertest": "^3.0.0"
    }
}
